def Smallest(arr, n):

	# Initialize maximum element
	min = arr[0]

	# Traverse array elements from second
	# and compare every element with
	# current max
	for i in range(1, n):
		if arr[i] > min:
			min = arr[i]
	return min

arr = [10, 324, 45, 90, 9808]
n = len(arr)
Ans = Smallest(arr, n)
print("Smallest in given array ", Ans)
